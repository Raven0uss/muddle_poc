type Message {
  id: ID! @id
  content: String!
  to: User! @relation(link: INLINE, name: "MessageTo")
  from: User! @relation(link: INLINE, name: "MessageFrom")
  conversation: Conversation! @relation(link: INLINE, name: "Conversation")
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}
